{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "sources/core/Stride.Core/Stride.Core.csproj",
          ],
          "src": "../../stride",
          "properties": {
            "TargetFramework": "net10.0",
            "StrideBuildDoc": "true"
          }
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Stride Docs",
      "_enableSearch": true,
      "_appLogoPath": "media/stride-logo-red.svg",
      "_appLogoUrl": "https://www.stride3d.net/",
      "_appFooter": "<div class=\"d-flex flex-column flex-sm-row justify-content-between pt-1 text-center small\"><p >Supported by the <a href=\"https://dotnetfoundation.org/\" target=\"_blank\" rel=\"noopener\">.NET Foundation</a></p><p>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></p><p >Stride Docs Website v.2.0.0.x</p><p>&copy; .NET Foundation and Contributors</p></div>",
      "_gitContribute": {
        "repo": "https://github.com/stride3d/stride-docs",
        "branch": "master"
      }
    },
    "fileMetadata": {
      "_appTitle": {
        "tutorials/**/*.md": "Stride tutorials",
        "manual/**/*.md": "Stride manual",
        "ReleaseNotes/**/*.md": "Stride release notes",
        "api/**/*.md": "Stride API",
        "api/**/*.yml": "Stride API",
        "diagnostics/**/*.md": "Stride diagnostics",
        "contributors/**/*.md": "Stride contributors",
        "community-resources/**/*.md": "Stride Community resources"
      }
    },
    "sitemap": {
      "baseUrl": "https://doc.stride3d.net/",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "tutorials/toc.yml",
          "tutorials/**/*.md",
          "ReleaseNotes/toc.yml",
          "ReleaseNotes/**/*.md",
          "manual/toc.yml",
          "manual/**/*.md",
          "contributors/toc.yml",
          "contributors/**/*.md",
          "diagnostics/toc.yml",
          "diagnostics/**/*.md",
          "community-resources/toc.yml",
          "community-resources/**/*.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "examples/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          ".nojekyll",
          "CNAME",
          "languages.json",
          "studio_getting_started_links.txt",
          "favicon.ico",
          "favicon.png",
          "images/**",
          "**/media/**/*.png",
          "**/media/**/*.jpg",
          "**/media/**/*.gif",
          "**/media/**/*.mp4",
          "**/media/**/*.zip",
          "**/media/**/*.svg",
          "**/media/**/*.webp",
          "**/media/**/*.cs",
          "contributors/engine/ndepend/**"
        ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "globalMetadataFiles": [],
    "markdownEngineProperties": {
      "toc": {
        "levels": "1,2,3"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
